---
export interface Props {
	title: string;
	description: string;
	publishDate: string;
	url: string;
}
const { title, description, publishDate, url } = Astro.props as Props;
---

<li>
    <strong><a href={url}>{title}</a></strong> -
    <span class="subt">{description}</span>
</li>

<style>
    .subt {
        color: var( --text-color-alt );
    }
</style>
